
{
  "version": 2,
  "engines": { 
    "python": "3.9" 
  },
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "maxLambdaSize": "15mb"
      }
    },
    {
      "src": "api/survey-agent.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "maxLambdaSize": "15mb"
      }
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "vite build",
        "outputDirectory": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/survey-agent/start",
      "dest": "/api/survey-agent.py"
    },
    {
      "src": "/api/survey-agent/process",
      "dest": "/api/survey-agent.py"
    },
    {
      "src": "/api/survey-agent/survey",
      "dest": "/api/survey-agent.py"
    },
    {
      "src": "/api/survey-agent/test",
      "dest": "/api/survey-agent.py"
    },
    {
      "src": "/api/((?!survey-agent).+)",
      "dest": "/api/index.py"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}