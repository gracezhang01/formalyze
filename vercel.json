{
  "version": 2,
  "builds": [
    {
      "src": "api/test.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "requirementsPath": "api/requirements.txt"
      }
    },
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "vite build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/test",
      "dest": "api/test.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}