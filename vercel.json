{
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm install && pip install -r requirements.txt",
  "outputDirectory": "dist",
  "framework": "vite",
  "routes": [
    { 
      "handle": "filesystem"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 